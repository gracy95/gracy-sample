package com.test;

import java.util.*;

public class Test {

	public static void main(String[] args) {

		String pattern = "Gracy";

		for (int i = 0; i < pattern.length(); i++) {
			for (int j = 0; j < pattern.length(); j++) {

				if (j == i || j == ((pattern.length() - 1) - i)) {
					System.out.print(pattern.charAt(j));
				} else {
					System.out.print(" ");
				}

			}
			System.out.println();
		}

	}

}
